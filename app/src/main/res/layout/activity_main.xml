<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/verticalRoad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:src="@drawable/ic_vertical_road"/>

    <ImageView
        android:id="@+id/horizontalRoad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/ic_horizontal_road"/>

    <ImageView
        android:id="@+id/light"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="60dp"
        app:layout_constraintLeft_toLeftOf="@id/verticalRoad"
        app:layout_constraintTop_toTopOf="@+id/horizontalRoad"
        android:src="@drawable/ic_baner"/>


    <ImageView
        android:id="@+id/tree_left_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="36dp"
        android:layout_marginBottom="136dp"
        android:src="@drawable/ic_tree_2"
        app:layout_constraintBottom_toBottomOf="@id/horizontalRoad"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/tree_left_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:src="@drawable/ic_tree_2"/>

    <ImageView
        android:id="@+id/tree_right_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="36dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:src="@drawable/ic_three_1"/>

    <ImageView
        android:id="@+id/supermarket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="136dp"
        android:layout_marginBottom="136dp"
        android:src="@drawable/ic_supermarket_1"
        app:layout_constraintBottom_toBottomOf="@id/horizontalRoad"
        app:layout_constraintLeft_toLeftOf="@id/verticalRoad" />

    <ImageView
        android:id="@+id/upImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="65dp"
        android:src="@drawable/ic_bus_vertical"/>

    <ImageView
        android:id="@+id/leftImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="55dp"
        android:src="@drawable/ic_blue_car"/>

    <ImageView
        android:id="@+id/rightImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="55dp"
        android:src="@drawable/ic_red_car"/>

    <ImageView
        android:id="@+id/downImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="65dp"
        android:src="@drawable/ic_blue_car_vertical"/>

    <LinearLayout
        android:id="@+id/trafficXOne"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/grey"
        android:orientation="vertical"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/light"
        app:layout_constraintVertical_bias="0.315">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/trafficXTwo"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/grey"
        android:orientation="vertical"
        android:layout_marginStart="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@id/trafficXOne"
        app:layout_constraintVertical_bias="0.274">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/trafficXThree"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/grey"
        android:orientation="vertical"
        android:layout_marginStart="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@id/trafficXOne"
        app:layout_constraintVertical_bias="0.548">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/trafficYOne"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/grey"
        android:orientation="vertical"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/light"
        app:layout_constraintVertical_bias="0.315">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/trafficYTwo"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/grey"
        android:orientation="vertical"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/trafficXOne"
        app:layout_constraintVertical_bias="0.274">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/trafficYThree"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/grey"
        android:orientation="vertical"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/trafficXOne"
        app:layout_constraintVertical_bias="0.548">
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>